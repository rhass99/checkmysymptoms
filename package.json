{
  "name": "symptom-checker",
  "engines": {
    "node": "8.12.0"
  },
  "version": "1.0.0",
  "description": "symptom checker API",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel ./src -s -d ./dist",
    "production": "npm run build && node ./bin/prod",
    "register": "NODE_ENV=dev node -r ./bin/dev",
    "babel-node": "NODE_ENV=dev babel-node --presets=env",
    "dev": "nodemon --exec npm run babel-node -- ./src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhass99/symptom-checker.git"
  },
  "author": "Rami Hassanein",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rhass99/symptom-checker/issues"
  },
  "homepage": "https://github.com/rhass99/symptom-checker#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "dotenv": "^6.0.0",
    "nodemon": "^1.18.4"
  }
}
